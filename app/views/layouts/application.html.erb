<!DOCTYPE html>
<html>
<head>
  <title>
    <% if content_for?(:title)%>
      <%= yield(:title) %>
    <% else %>
      ProjectRobin.org
    <% end %>
  </title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>

<body>
  
  <%= render partial: 'layouts/header' %>

  <div class="container">

    <%= flash_messages %>

    <%= yield %>
  </div>
  
</body>
</html>
